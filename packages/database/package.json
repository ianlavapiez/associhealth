{
  "name": "@workspace/database",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=22.0.0"
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./schema": "./src/schema/index.ts",
    "./types": "./src/types/index.ts",
    "./migrations": "./src/migrations/index.ts"
  },
  "dependencies": {
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.4",
    "drizzle-orm": "^0.44.5",
    "next": "^15.5.3",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@workspace/typescript-config": "workspace:*",
    "drizzle-kit": "^0.31.4",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsc",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "examples": "tsx --env-file=.env src/lib/examples.ts",
    "test:crypto": "tsx --env-file=.env src/lib/test-crypto.ts"
  }
}
